{
	"name": "filter",
	"description": "Simple Qwik filter container component",
	"scripts": {
		"prebuild": "npm run -w helpers build",
		"build": "npm run build.client && npm run build.ssr",
		"build.client": "vite build",
		"build.ssr": "vite build --ssr src/entry.ssr.tsx",
		"lint": "eslint \"src/**/*.ts*\"",
		"check-format": "prettier --check --ignore-path=../.prettierignore .",
		"format": "prettier --write --ignore-path=../.prettierignore .",
		"start": "npx wrangler dev --local --env test",
		"deploy": "npx wrangler publish",
		"deploy:test": "npx wrangler publish --env test",
		"deploy:prod": "npx wrangler publish --env prod",
		"typecheck": "tsc --incremental --noEmit"
	},
	"devDependencies": {
		"@builder.io/qwik": "^1.1.4",
		"@cloudflare/workers-types": "^3.14.1",
		"@types/eslint": "8.4.5",
		"@types/express": "4.17.13",
		"@types/node": "latest",
		"@typescript-eslint/eslint-plugin": "5.30.6",
		"@typescript-eslint/parser": "5.30.6",
		"eslint": "8.20.0",
		"eslint-plugin-qwik": "1.1.4",
		"prettier": "2.7.1",
		"typescript": "5.1.3",
		"vite": "3.0.1",
		"vite-tsconfig-paths": "3.5.0"
	},
	"engines": {
		"node": ">=16"
	},
	"license": "Apache-2.0",
	"private": true,
	"type": "module"
}
